{
  "manifest_version": 2,
  "name": "openapi-forms-ui-web-ext",
  "description": "description",
  "version": "0.0.1",
  "icons": {
    "64": "assets/icons/icon.png"
  },
  "background": {
    "scripts": ["background_script.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["content_script.js"]
    }
  ],
  "browser_action": {
    "default_icon": {
      "64": "assets/icons/icon.png"
    }
  },
  "options_ui": {
    "page": "options.html"
  }
}
