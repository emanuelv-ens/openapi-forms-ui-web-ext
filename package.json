{
  "name": "openapi-forms-ui-web-ext",
  "version": "0.0.2",
  "private": true,
  "description": "This web extension provides a user-friendly interface for interacting with OpenAPI specifications. It allows users to view and execute API operations, manage request parameters, and handle request bodies using dynamically generated forms.",
  "scripts": {
    "webpack": "node --loader ts-node/esm -- node_modules/webpack/bin/webpack.js --",
    "build": "pnpm run build:development",
    "build:development": "pnpm run webpack --config webpack.development.ts",
    "build:production": "pnpm run webpack --config webpack.production.ts",
    "watch": "pnpm run watch:development",
    "watch:development": "pnpm run build:development --watch",
    "watch:production": "pnpm run build:production --watch",
    "build-ext": "web-ext build",
    "start:firefox": "web-ext run --target firefox-desktop",
    "start:chrome": "web-ext run --target chromium --config web-ext-chrome.config.mjs",
    "start:edge": "web-ext run --target chromium --config web-ext-edge.config.mjs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@9.9.0+sha512.60c18acd138bff695d339be6ad13f7e936eea6745660d4cc4a776d5247c540d0edee1a563695c183a66eb917ef88f2b4feb1fc25f32a7adcadc7aaf3438e99c1",
  "engines": {
    "pnpm": ">=9.0.0 <10.0.0",
    "node": ">=20.0.0 <21.0.0"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^11.7.0",
    "@jsonforms/core": "^3.3.0",
    "@jsonforms/material-renderers": "^3.3.0",
    "@jsonforms/react": "^3.3.0",
    "@zenfs/core": "^0.16.2",
    "ajv": "8.16.0",
    "ajv-formats": "^3.0.1",
    "buffer": "^6.0.3",
    "https-browserify": "^1.0.0",
    "oas": "^24.5.3",
    "oas-normalize": "^11.1.1",
    "path-browserify": "^1.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "stream-http": "^3.2.0",
    "styled-components": "^6.1.12",
    "url": "^0.11.4",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.8.0",
    "@types/gulp": "^4.0.17",
    "@types/gulp-zip": "^4.0.4",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/webextension-polyfill": "^0.10.7",
    "copy-webpack-plugin": "^12.0.2",
    "dotenv": "^16.4.5",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.9.0",
    "html-webpack-plugin": "^5.6.0",
    "prettier": "^3.3.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.0",
    "typescript-plugin-styled-components": "^3.0.0",
    "web-ext": "^8.2.0",
    "web-ext-plugin": "^2.10.0",
    "webextension-polyfill": "^0.12.0",
